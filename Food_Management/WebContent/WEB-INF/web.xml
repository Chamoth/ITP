<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>FoodManage</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
		<servlet-name>addfoodservlet</servlet-name>
		<servlet-class>food_management.AddFoodServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>addfoodservlet</servlet-name>
		<url-pattern>/addfood</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>updatefoodservlet</servlet-name>
		<servlet-class>food_management.UpdateFoodServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updatefoodservlet</servlet-name>
		<url-pattern>/updatefood</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>deletefoodservlet</servlet-name>
		<servlet-class>food_management.DeleteFoodServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deletefoodservlet</servlet-name>
		<url-pattern>/deletefood</url-pattern>
	</servlet-mapping>
  
</web-app>