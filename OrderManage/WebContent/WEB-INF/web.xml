<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
version="3.1">

<servlet>
	<servlet-name>login</servlet-name>
	<servlet-class>com.order.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>login</servlet-name>
	<url-pattern>/log</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>orderInsert</servlet-name>
	<servlet-class>com.order.orderInsert</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>orderInsert</servlet-name>
	<url-pattern>/insert</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>orderUpdate</servlet-name>
	<servlet-class>com.order.UpdateOrderServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>orderUpdate</servlet-name>
	<url-pattern>/update</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>orderDelete</servlet-name>
	<servlet-class>com.order.DeleteOrderServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>orderDelete</servlet-name>
	<url-pattern>/delete</url-pattern>
</servlet-mapping>

</web-app>

